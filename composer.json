{
    "name": "somnambulist/collection",
    "type": "library",
    "description": "Collection implementation with no dependencies.",
    "keywords": [
        "domain",
        "collection"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dave Redfern",
            "email": "dave@scorpioframework.com"
        }
    ],
    "require": {
        "php": ">=7"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.2"
    },
    "autoload": {
        "psr-4": {
            "Somnambulist\\Collection\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Somnambulist\\Test\\Collection\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "bin-dir": "bin"
    }
}
